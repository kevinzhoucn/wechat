{% extends 'AcmeWebBundle:Layout:wechat_base.html.twig' %}

{% block title %}设置成功{% endblock %}

{% block body_content %}
    <div class="container" id="container">
        <div class="msg">
            <div class="weui_msg">
                <div class="weui_icon_area">
                    <i class="weui_icon_success weui_icon_msg"></i>
                </div>
                <div class="weui_text_area">
                    <h2 class="weui_msg_title">绑定成功</h2>
                </div>
                <div class="weui_opr_area">
                    <p class="weui_btn_area">
                         <a href="javascript:;" class="weui_btn weui_btn_primary" id="a_bind_confirm">确定</a>
                    </p>
                </div>
                <div class="weui_extra_area">
                </div>
            </div>
        </div>
    </div>
{% endblock %}

{% block body_javascript %}
  {{ parent() }}  
  <script src="/js/wechat-config.js"> </script>
  <script src="/js/app_bind.js"> </script>
{% endblock %}
